class Solution {
    public int rob(int[] nums) {
        int n=nums.length;
        int prev=nums[0];
        int prev2=0;
        int curri=0;
        for(int i=1;i<n;i++)
        {
            int take=nums[i];
            if(i>1)
            take+=prev2;
            int ntake=0+prev;
            curri=Math.max(take,ntake);
            prev2=prev;
            prev=curri;
        }
        return prev;
    }
}
        
